# RabbitMQ Configuration
RABBITMQ_URL=amqp://rabbitmq:password@rabbitmq:5672/
REQUEST_QUEUE=gemini.requests
RESPONSE_QUEUE=gemini.responses

# Gemini API Keys (comma-separated)
GEMINI_API_KEYS=your_key_1,your_key_2

# Default Gemini model for text generation
GEMINI_MODEL_TEXT=gemini-2.5-flash

# Rate Limiting Configuration
KEYS_MAX_PER_MINUTE=10
KEYS_COOLDOWN_SECONDS=60

# Queue Retry Configuration (escalating delays in seconds)
QUEUE_RETRY_DELAYS=60,600,3600,86400
QUEUE_MAX_RETRIES=4

# HTTP Proxy (optional)
# HTTP_PROXY=http://localhost:8080
# HTTPS_PROXY=http://localhost:8080

# Logging Configuration
LOG_LEVEL=INFO

# Worker Configuration
WORKER_PREFETCH_COUNT=10
